<div class="modal fade" id="modal-detail-about" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true"
    style="margin-top: 23px;">
    <div class="modal-dialog modal-lg" style="margin-top: -60px">
        <div class="modal-content" style="margin: 59px 0;">
            <div class="modal-header">
                <h5 class="modal-title text-uppercase fs-14" id="exampleModalLabel">Thông tin</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body" style="padding: 0 14px;">
                <form action="user/updateProfile" method="POST" enctype='multipart/form-data'>
                    <div class="container shadow">
                        <div class="row">
                            <div class="col-12 p-0">
                                <div>
                                    <img src="https://cover-talk.zadn.vn/2/c/8/7/3/1b000c3d1edecee9e0fd6c2ceca6616c.jpg"
                                        alt="Avatar-logo" style=" position: relative;
                                        top: 0px;
                                        left: 0px;
                                        width: 498px;
                                        height: 180px;
                                        object-fit: cover;
                                        width: 100%
                                        ">
                                    <div class="rel" style="text-align: center; margin: -47px 0px 0px;">
                                        <img src="https://stores3appchatmobile152130-dev.s3.ap-southeast-1.amazonaws.com/public/{{user.avatar}}"
                                            style="border-radius: 50%; width: 80px; height: 80px; object-fit: cover;
                                            cursor: pointer; position: relative" id="avatar">

                                    </div>
                                </div>
                                <div class="position-absolute avatar-camera cursor-point">
                                    <label for="choose-image"><i class="far fa-camera-alt"></i>
                                    </label>
                                    <input type="file" accept="image/*" onchange="preview_image(event)"
                                        id="choose-image" class="input-hidden" name="avatar">
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-12 p-0" id="editor-name">

                            <div id="editor-name-parent">
                                <h2 class="text-center border-bottom mb-0 p-2 txt-name-default">
                                    <span> {{user.userName}}</span><i
                                        class="fal fa-edit fs-14 ms-4 cursor-point edit"></i>
                                </h2>

                            </div>

                        </div>

                        <div class="col-12 border-bottom p-4">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="mb-3 fs-14">
                                        <label for="" class="pb-2 text-white">Số điện thoại đã đăng ký</label>
                                        <input type="text" class="form-control fs-14 no-drop" id="phone"
                                            placeholder="{{user.local.phone}}" readonly />
                                    </div>
                                </div>
                                <label for="" class="pb-2 fs-14 text-white">Ngày sinh</label>
                                <div class="col-3">
                                    <select class="form-select mb-3 fs-14 text-white" id="ddlNgay">
                                    </select>
                                </div>
                                <div class="col-3">
                                    <select class="form-select mb-3 fs-14 text-white" id="ddlThang"
                                        onchange="loadDay()">
                                    </select>
                                </div>
                                <div class="col-3">
                                    <select class="form-select mb-3 fs-14 text-white" id="ddlNam" onchange="loadDay()">
                                    </select>
                                    <input type="hidden" class="form-control fs-14" id="birthday" name="birthday"
                                        value="{{user.birthday}}" onchange="loadBirthday()" />

                                </div>
                                <div class="col-md-12">
                                    <input type="hidden" id="userName" value="{{user.userName}}">
                                    <input type="hidden" name="id" id="id" value="{{user._id}}">
                                    <input type="hidden" id="gender" value="{{user.gender}}">
                                    <div class="col-12 fs-14">
                                        <div>
                                            <label for="" class="pb-2 text-white">Giới tính</label>
                                            <div class="custom-control custom-radio">
                                                <input type="radio" id="radio1" name="gender"
                                                    class="custom-control-input" value="male">
                                                <label class="custom-control-label text-white" for="radio1">Nam</label>

                                                <input type="radio" id="radio2" name="gender"
                                                    class="custom-control-input" value="female">
                                                <label class="custom-control-label text-white" for="radio2">Nữ</label>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="modal-footer d-block">
                                <button type="submit" class="btn btn-danger form-control form-control-sm fs-14
                                                                        text-white text-uppercase"
                                    onclick="loadBirthday()">Lưu</button>
                            </div>
                        </div>
                </form>
            </div>
        </div>

    </div>
</div>